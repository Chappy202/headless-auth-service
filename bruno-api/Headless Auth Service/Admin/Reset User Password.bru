meta {
  name: Reset User Password
  type: http
  seq: 2
}

put {
  url: {{url}}/admin/users/:userId/reset-password
  body: json
  auth: bearer
}

params:path {
  userId: 1
}

headers {
  Content-Type: application/json
}

auth:bearer {
  token: {{access_token}}
}

body:json {
  {
    "newPassword": "NewStrongPassword123"
  }
}
